
class Point1 {
	double x;
	double y;
	public Point1 (double x, double y){
		this.x = x;
		this.y = y;
	}
}

public class doOverLap {
	
	public boolean doOverlap (Point1 topLeftA, Point1 topLeftB, Point1 bottomRightA, Point1 bottomRightB) {
		//A above B or B above A
		if (bottomRightA.y >= topLeftB.y || bottomRightB.y >= topLeftA.y ) {
			return false;
		}
		//A to the left of B or B to the left of A
		if (bottomRightA.x <= topLeftB.x || bottomRightB.x <= topLeftA.x) {
			return false;
		}
		return true;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		doOverLap dooverlap = new doOverLap();
		Point1 topLeftA = new Point1(0, 1.1);
		Point1 bottomRightA = new Point1(1, 0);
		Point1 topLeftB = new Point1(0.9, 2);
		Point1 bottomRightB = new Point1(2, 1);
		System.out.println(dooverlap.doOverlap(topLeftA, topLeftB, bottomRightA, bottomRightB));
	}

}
