import java.util.*;

class Node {
	int val;
	ArrayList<Node> children;
	public Node (int val) {
		this.val = val;
		children = new ArrayList<Node>();
	}
}

class SumCount {
	int sum;
	int count;
	public SumCount (int sum, int count) {
		this.sum = sum;
		this.count = count;
	}
}

public class Solution1 {

	public static void main(String[] args) {
